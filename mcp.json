{
  "mcpServers": {
    "gcloud": {
      "command": "npx",
      "args": ["-y", "@google-cloud/gcloud-mcp"]
    },
    "observability": {
      "command": "npx",
      "args": ["-y", "@google-cloud/observability-mcp"]
    },
    "storage": {
      "command": "npx",
      "args": ["-y", "@google-cloud/storage-mcp"]
    },
    "cloud-run": {
      "command": "npx",
      "args": ["-y", "@google-cloud/cloud-run-mcp"]
    },
    "gke": {
      "command": "gke-mcp"
    },
    "toolbox": {
      "command": "toolbox",
      "args": [
        "--tools-file",
        "tools.yaml"
      ]
    },
    "nvidia-blog-agent": {
      "command": "python",
      "args": ["-u", "run_mcp_server.py"],
      "env": {
        "NVIDIA_BLOG_SERVICE_URL": "https://nvidia-blog-agent-yuav3bbrka-uc.a.run.app",
        "INGEST_API_KEY": "emik6JhmdDhsVCHxjfRqz1tgDEnxljPEWh2WTXwyPMw"
      }
    },
    "secops": {
      "command": "uvx",
      "args": [
        "--from",
        "google-secops-mcp",
        "secops_mcp"
      ],
      "env": {
        "CHRONICLE_PROJECT_ID": "",
        "CHRONICLE_CUSTOMER_ID": "",
        "CHRONICLE_REGION": "us"
      }
    },
    "gti": {
      "command": "uvx",
      "args": [
        "gti_mcp"
      ],
      "env": {
        "VT_APIKEY": ""
      }
    },
    "scc-mcp": {
      "command": "uvx",
      "args": [
        "scc_mcp"
      ],
      "env": {}
    },
    "secops-soar": {
      "command": "uvx",
      "args": [
        "secops_soar_mcp"
      ],
      "env": {
        "SOAR_URL": "",
        "SOAR_APP_KEY": ""
